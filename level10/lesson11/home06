package com.javarush.test.level10.lesson11.home06;

/* Конструкторы класса Human
Напиши класс Human с 6 полями. Придумай и реализуй 10 различных конструкторов для него. Каждый конструктор должен иметь смысл.
*/

public class Solution
{
    public static void main(String[] args)
    {

    }

    public static class Human
    {
        //напишите тут ваши переменные и конструкторы
        String name, familyName;
        int age, weight, height;
        boolean sex;

        public Human(String name)
        {
            this.name = name;
        }

        public Human(int age)
        {
            this.age = age;
        }

        public Human(boolean sex)
        {
            this.sex = sex;
        }

        public Human(String name, String familyName, int weight, int height, int age, boolean sex)
        {
            this.name = name;
            this.familyName = familyName;
            this.age = age;
            this.weight = weight;
            this.height = height;
            this.sex = sex;
        }

        public Human(String name, String familyName)
        {
            this.name = name;
            this.familyName = familyName;
        }

        public Human(int weight, int height)
        {
            this.weight = weight;
            this.height = height;
        }

        public Human(String name, boolean sex, Human father)
        {
            this.name = name;
            this.familyName = father.familyName;
            this.sex = sex;
        }

        public Human(int age, String name)
        {
            this.age = age;
            this.name = name;
        }

        public Human(String name, String familyName, int age)
        {
            this.name = name;
            this.familyName = familyName;
            this.age = age;
        }

        public Human(String name, String familyName, boolean sex, int age)
        {
            this.name = name;
            this.familyName = familyName;
            this.age = age;
            this.sex = sex;
        }
    }
}
