    public static class Human
    {
        //напишите тут ваши переменные и конструкторы
        private String name = "";
        private String familyName = "";
        private int age, weight, height;
        private Human father;

        public void setName (String name) {
            this.name = name;
        }

        public void setFamilyName (String familyName) {
            this.familyName = familyName;
        }

        public void setFullName (String name, String familyName) {
            this.name = name;
            this.familyName = familyName;
        }

        public void setWeight (int weight)
        {
            this.weight = weight;
        }

        public void setHeight (int height) {
            this.height = height;
        }

        public void setAge (int age) {
            this.age = age;
        }

        public void setMeasures (int height, int weight) {
            this.height = height;
            this.weight = weight;
        }

        public void setFather (Human father) {
            this.father = father;
        }

        public void initialize(String name, int age) {
            this.name = name;
            this.age = age;
        }

        public void fullData (String name, String familyName, int height, int weight, int age, Human father) {
            this.name = name;
            this.familyName = familyName;
            this.age = age;
            this.height = height;
            this.weight = weight;
            this.father = father;
        }

    }
}
